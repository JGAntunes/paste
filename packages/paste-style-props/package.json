{
  "name": "@twilio-paste/style-props",
  "version": "1.8.15",
  "description": "Shared utilities for style properties used in Paste",
  "author": "Twilio Inc.",
  "license": "MIT",
  "main:dev": "src/index.ts",
  "main": "dist/twilio-paste-style-props.cjs.js",
  "module": "dist/twilio-paste-style-props.esm.js",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build:props": "typedoc --tsconfig ./tsconfig.json --json ./dist/prop-types.json",
    "clean": "rm -rf ./dist && rm -rf tsconfig.build.tsbuildinfo && rm -rf .rpt2_cache",
    "compile": "rollup -c --environment NODE_ENV:production",
    "compile:dev": "rollup -c --environment NODE_ENV:development"
  },
  "dependencies": {
    "csstype": "^2.6.10"
  },
  "peerDependencies": {
    "@babel/runtime": "^7.12.5",
    "@twilio-paste/design-tokens": "^6.4.1",
    "@twilio-paste/styling-library": "^0.2.1",
    "@twilio-paste/theme": "^4.1.1",
    "prop-types": "^15.7.2"
  },
  "devDependencies": {
    "@babel/runtime": "^7.12.5",
    "@twilio-paste/design-tokens": "^6.4.1",
    "@twilio-paste/styling-library": "^0.2.1",
    "@twilio-paste/theme": "^4.1.1",
    "prop-types": "^15.7.2"
  }
}
